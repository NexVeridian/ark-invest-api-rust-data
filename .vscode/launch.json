	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'ark-invest-api-rust-data'",
			"cargo": {
				"args": [
					"build",
					"--bin=ark-invest-api-rust-data",
					"--package=ark-invest-api-rust-data"
				],
				"filter": {
					"name": "ark-invest-api-rust-data",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'ark-invest-api-rust-data'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=ark-invest-api-rust-data",
					"--package=ark-invest-api-rust-data"
				],
				"filter": {
					"name": "ark-invest-api-rust-data",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}