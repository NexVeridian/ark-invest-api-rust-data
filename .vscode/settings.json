{
	"[Rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.formatOnSave": true,
		"editor.formatOnSaveMode": "file"
	},
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.cargo.buildScripts.overrideCommand": [
		"cargo",
		"clippy",
		"--fix",
		"--workspace",
		"--message-format=json",
		"--all-targets",
		"--allow-dirty"
	],
}